  ^\s*Allowed\sVLAN:\s+(\d+\(\w+\),\s*){4}${ALLOWED_VLAN}\(${ALLOWED_VLAN_TAG}\),\s* -> Continue
  ^\s*Allowed\sVLAN:\s+(\d+\(\w+\),\s*){5}${ALLOWED_VLAN}\(${ALLOWED_VLAN_TAG}\),\s* -> Continue
  ^\s*Allowed\sVLAN:\s+(\d+\(\w+\),\s*){6}${ALLOWED_VLAN}\(${ALLOWED_VLAN_TAG}\),\s* -> Continue
  ^\s*Allowed\sVLAN:\s+(\d+\(\w+\),\s*){7}${ALLOWED_VLAN}\(${ALLOWED_VLAN_TAG}\),\s* -> Continue
  ^\s*Allowed\sVLAN:\s+(\d+\(\w+\),\s*){8}${ALLOWED_VLAN}\(${ALLOWED_VLAN_TAG}\),\s* -> Continue
